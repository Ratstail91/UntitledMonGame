{
    "compilerOptions": {
      "lib": ["esnext", "DOM"],
      "experimentalDecorators": true,
      "allowJs": true,
      "jsx": "react",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "target": "ES6",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "module": "esnext",
      "useDefineForClassFields":true, 
    },
    "exclude": ["node_modules/*"]
  }
  