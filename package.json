{
  "name": "EggTrainer",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "forever -a -o forever.log -e error.log start build/index.js",
    "restart": "forever -a -o forever.log -e error.log restart build/index.js",
    "stop": "forever stop build/index.js",
    "node": "node build/index.js",
    "build": "npm run webpack-production && npm run build:server",
    "build:server": "tsc --build server/tsconfig.json",
    "webpack": "webpack --mode=development --env=development --config webpack.config.js",
    "webpack-production": "webpack --mode=production --env=production --config webpack.config.js",
    "pull": "git pull && npm run webpack-production && npm restart",
    "dev": "concurrently npm:watch:server npm:watch:client",
    "analyzer": "webpack --mode=production --env=analyzer --config webpack.config.js",
    "watch:server": "ts-node-dev --project server/tsconfig.json --respawn server/index.ts",
    "watch:client": "webpack-dev-server --mode=development --env=development --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ratstail91/EggTrainer.git"
  },
  "author": "Kayne Ruse",
  "license": "N/a",
  "bugs": {
    "url": "https://github.com/Ratstail91/EggTrainer/issues"
  },
  "homepage": "https://github.com/Ratstail91/EggTrainer#readme",
  "browserslist": [
    "Chrome >= 60",
    "Firefox >= 70"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "braintree": "^2.23.0",
    "braintree-web": "^3.62.1",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firstline": "^2.0.2",
    "forever": "^3.0.0",
    "formidable": "^1.2.2",
    "mysql2": "^2.1.0",
    "paypal-checkout": "^4.0.314",
    "query-string": "^6.12.1",
    "react-markdown": "^4.3.1",
    "sendmail": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.6",
    "@types/formidable": "^1.0.31",
    "@types/mysql2": "github:types/mysql2",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
