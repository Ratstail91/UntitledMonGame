{
  "name": "EggTrainer",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "forever -a -o forever.log -e error.log start server/index.js",
    "restart": "forever -a -o forever.log -e error.log restart server/index.js",
    "stop": "forever stop server/index.js",
    "node": "node server/index.js",
    "webpack": "webpack --mode=development --env=development",
    "webpack-production": "webpack --mode=production --env=production",
    "pull": "git pull && npm run webpack-production && npm restart",
    "dev": "concurrently npm:watch:server npm:watch:client",
    "db:create": "node scripts/createdb.js",
    "db:clean": "node scripts/cleandb.js",
    "watch:server": "nodemon --watch server server/index.js",
    "watch:client": "webpack-dev-server --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ratstail91/EggTrainer.git"
  },
  "author": "Kayne Ruse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ratstail91/EggTrainer/issues"
  },
  "homepage": "https://github.com/Ratstail91/EggTrainer#readme",
  "browserslist": [
    "Chrome >= 60",
    "Firefox >= 70"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cron": "^1.7.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firstline": "^2.0.2",
    "forever": "^2.0.0",
    "formidable": "^1.2.1",
    "mysql2": "^2.1.0",
    "query-string": "^6.9.0",
    "react-markdown": "^4.3.1",
    "sendmail": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.2",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
